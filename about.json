{
  "name": "Topic List Thumbnails",
  "component": true,
  "authors": "David Taylor",
  "about_url": "https://meta.discourse.org/t/topic-list-thumbnails/150602",
  "license_url": null,
  "minimum_discourse_version": "2.5.0.beta4",
  "modifiers": {
    "csp_extensions": ["script_src: https://unpkg.com"],
    "svg_icons": ["images"],
    "serialize_topic_excerpts": true,
